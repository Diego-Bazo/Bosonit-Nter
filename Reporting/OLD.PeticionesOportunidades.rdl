    <DataSet Name="Usuario_Car">
      <Query>
        <DataSourceName>SLQ_Server_Bosonit_ODS</DataSourceName>
        <CommandText>SELECT usuario.[id]
      ,usuario.[email]
	  ,REPLACE ( usuario.[email] , '@bosonit.com' , '' ) as usuario
	  ,cliente.CAR
  FROM [ODS_BOSONITBI].[crm].[usuarios] usuario left join [ODS_BOSONITBI].[crm].[clientes] cliente on cliente.id_responsable_cuenta=usuario.id</CommandText>
