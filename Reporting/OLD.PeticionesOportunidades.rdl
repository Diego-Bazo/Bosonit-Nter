          <QueryParameter Name="@ESTADO_OPORTUNIDAD">
            <Value>=Parameters!ESTADO_OPORTUNIDAD.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT usuario.[id] id_usuario
      ,usuario.[email] email_usuario
	  ,left(usuario.email, charindex('@', usuario.email) - 1) correo_usuario
	  ,usuario.nombre nombre_usuario
	  ,usuario.apellidos aperllidos_usuario
	  ,cliente.id id_cliente
	  ,cliente.[cif_nif] cif_cliente
      ,cliente.[nombre] nombre_cliente
      ,cliente.[descripcion] descripcion_cliente
      ,cliente.[CAR]
      ,cliente.[ciudad] ciudad_cliente
      ,cliente.[sector] sector_cliente
      ,cliente.[web] web_cliente
	  ,cliente.id_responsable_cuenta
	  ,peticiones.id id_peticion
	  ,peticiones.nombre nombre_peticion
	  ,peticiones.id_estado estado_peticion
	  ,pipe2.nombre_estado nombre_estado_peticion
	  ,pipe2.orden orden_peticion
	  ,pipe2.probabilidad probabilidad_peticion
	  ,peticiones.prioridad prioridad_peticion
	  ,peticiones.cantidad cantidad_peticion
,peticiones.[id_tipo_oportunidad]
	  ,oportunidades.id id_oportunidad
	  ,oportunidades.titulo titulo_oportunidad
	  ,oportunidades.descripcion descripcion_oportunidad
	  ,oportunidades.business_line 
	  ,oportunidades.id_estado estado_oportunidad
	  ,oportunidades.prioridad prioridad_oportunidad
	  ,pipe.nombre_estado nombre_estado_oportunidad
	  ,pipe.orden orden_oportunidad
	  ,pipe.probabilidad probabilidad_oportunidad
  FROM [ODS_BOSONITBI].[crm].[clientes] cliente
  left join [ODS_BOSONITBI].[crm].[usuarios] usuario  on cliente.id_responsable_cuenta=usuario.id
  left join [ODS_BOSONITBI].[crm].[clientes_perfiles] perfiles on perfiles.id_crm_principal=cliente.id
  left join [ODS_BOSONITBI].[crm].[peticiones] peticiones on peticiones.id_cliente=perfiles.id
  left join [ODS_BOSONITBI].[crm].[oportunidades] oportunidades on oportunidades.id_cliente=cliente.id
  left join [ODS_BOSONITBI].[crm].[pipelines] pipe on pipe.id_estado=oportunidades.id_estado
  left join [ODS_BOSONITBI].[crm].[pipelines_perfiles] pipe2 on pipe2.id_estado=peticiones.id_estado
where usuario.[id]=@USUARIO AND cliente.[CAR] IN (@CAR) AND isnull(peticiones.id_estado,'NULL') in (@ESTADO_PETICION) AND isnull(oportunidades.id_estado,'NULL') in (@ESTADO_OPORTUNIDAD)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
